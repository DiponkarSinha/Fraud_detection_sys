{
  "title": "Real-time Fraud Detection Dataset",
  "id": "diponkarsinha/fraud-detection-realtime",
  "licenses": [
    {
      "name": "CC0-1.0"
    }
  ],
  "keywords": [
    "fraud detection",
    "machine learning",
    "financial transactions",
    "anomaly detection",
    "real-time data",
    "automated pipeline"
  ],
  "collaborators": [],
  "data": [
    {
      "description": "Raw transaction data with fraud labels",
      "name": "raw",
      "totalBytes": 0,
      "columns": [
        {
          "name": "transaction_id",
          "description": "Unique identifier for each transaction",
          "type": "string"
        },
        {
          "name": "user_id",
          "description": "Unique identifier for the user",
          "type": "string"
        },
        {
          "name": "amount",
          "description": "Transaction amount in USD",
          "type": "numeric"
        },
        {
          "name": "merchant_category",
          "description": "Category of merchant (grocery, electronics, etc.)",
          "type": "string"
        },
        {
          "name": "location",
          "description": "Transaction location",
          "type": "string"
        },
        {
          "name": "time_hour",
          "description": "Hour of the day when transaction occurred (0-23)",
          "type": "numeric"
        },
        {
          "name": "day_of_week",
          "description": "Day of the week (1=Monday, 7=Sunday)",
          "type": "numeric"
        },
        {
          "name": "is_weekend",
          "description": "Whether transaction occurred on weekend (0=No, 1=Yes)",
          "type": "numeric"
        },
        {
          "name": "previous_failed_attempts",
          "description": "Number of previous failed transaction attempts",
          "type": "numeric"
        },
        {
          "name": "account_age_days",
          "description": "Age of user account in days",
          "type": "numeric"
        },
        {
          "name": "avg_transaction_amount",
          "description": "User's average transaction amount",
          "type": "numeric"
        },
        {
          "name": "transaction_frequency",
          "description": "User's transaction frequency per month",
          "type": "numeric"
        },
        {
          "name": "is_fraud",
          "description": "Fraud label (0=Legitimate, 1=Fraud)",
          "type": "numeric"
        }
      ]
    }
  ]
}